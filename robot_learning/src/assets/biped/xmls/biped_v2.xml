          <joint name="R_HAA" pos="0 0 0" axis="1 0 0" limited="true" range="-0.25 0.25"/>
          <geom name="R_SHOULDER" type="mesh" contype="0" conaffinity="0" rgba="1 0 0 1" mesh="R_SHOULDER"/>
          
          <body name="R_UPPER_LEG" pos="0 0 0">
