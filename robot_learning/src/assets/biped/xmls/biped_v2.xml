            <joint name="R_HFE" pos="0 0 0" axis="0 1 0" limited="true" range="-1.5 1.5"/>
            <geom name="R_UPPER_LEG" type="mesh" contype="0" conaffinity="0" rgba="1 0 0 1" mesh="R_UPPER_LEG"/>
            
